<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>prometheus.backup.obsidian</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Users/prometheus/Workspace/dotfiles/launchd/backup-obsidian/backup_obsidian.sh</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardErrorPath</key>
    <string>/tmp/backup-obsidian.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/backup-obsidian.log</string>
    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>15</integer>
      <key>Minute</key>
      <integer>05</integer>
      <key>Weekday</key>
      <integer>7</integer>
    </dict>
  </dict>
</plist>